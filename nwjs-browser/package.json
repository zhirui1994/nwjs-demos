{
  "main": "browser.html",
  "name": "nwjs-browser-framework",
  "description": "nwjs configureable browser framework",
  "version": "0.0.1",
  "nodejs": true,
  "keywords": [
    "browser",
    "nwjs",
    "node-webkit"
  ],
  "chromium-args": "--remember-cert-error-decisions --ignore-certificate-errors --ignore-urlfetcher-cert-requests",
  "window": {
    "title": " ",
    "icon": "icons/browser-128.png",
    "toolbar": false,
    "frame": true,
    "width": 1024,
    "height": 700,
    "min_width": 250,
    "min_height": 250
  },
  "webkit": {
    "plugin": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/RIAEvangelist/nwjs-browser-framework.git"
  },
  "author": "Brandon Nozaki Miller",
  "license": "DBAD",
  "bugs": {
    "url": "https://github.com/RIAEvangelist/nwjs-browser-framework/issues"
  }
}
